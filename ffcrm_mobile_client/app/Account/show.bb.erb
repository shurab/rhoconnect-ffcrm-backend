<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @account.user_id %></h1>
    <%= link_to "Edit", :action => :edit, :id => @account.object %>
  </div>

  <div data-role="content">
    <table>
      <tr>
    	<td class="itemLabel">UserId</td>
        <td class="itemValue"><%= @account.user_id%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">AssignedTo</td>
        <td class="itemValue"><%= @account.assigned_to%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Name</td>
        <td class="itemValue"><%= @account.name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Email</td>
        <td class="itemValue"><%= @account.email%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">CreatedAt</td>
        <td class="itemValue"><%= @account.created_at%></td>
      </tr>
      
    </table>
  </div>

</div>
