<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @contact.object %>
    <h1>Edit <%= @contact.user_id%></h1>
    <%= link_to "Delete", :action => :delete, :id => @contact.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @contact.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">User: </td>
              <td class="itemValue"><input type="text" name="contact[user_id]" value="<%= @contact.user_id %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Lead: </td>
              <td class="itemValue"><input type="text" name="contact[lead_id]" value="<%= @contact.lead_id %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Assigned to: </td>
              <td class="itemValue"><input type="text" name="contact[assigned_to]" value="<%= @contact.assigned_to %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">First name: </td>
              <td class="itemValue"><input type="text" name="contact[first_name]" value="<%= @contact.first_name %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Last name: </td>
              <td class="itemValue"><input type="text" name="contact[last_name]" value="<%= @contact.last_name %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Title: </td>
              <td class="itemValue"><input type="text" name="contact[title]" value="<%= @contact.title %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Email: </td>
              <td class="itemValue"><input type="text" name="contact[email]" value="<%= @contact.email %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Created at: </td>
              <td class="itemValue"><input type="text" name="contact[created_at]" value="<%= @contact.created_at %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
